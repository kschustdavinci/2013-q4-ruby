<h2>Elements</h2>

<ul>
  <% Element.order(:id).each do |e| %>
    <li>
      <a href="/elements/<%= e.id %>">
        <%= e.name %>
      </a>
    </li>
  <% end %>
</ul>

<a href="/create_element"><u>Create new element</u></a>

<h2>World Cities</h2>

<ul>
  <% WorldCity.order(:id).each do |c| %>
    <li>
      <a href="/cities/<%= c.id %>">
        <%= c.name %>
      </a>
    </li>
    <li>
      <a href="/cities/<%= c.id %>">
        <%= c.population_in_millions %>
      </a>
    </li>
  <% end %>
</ul>

<a href="/population_double"><u>Double Population of Paris</u></a>
